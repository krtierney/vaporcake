      <li><%= link_to 'Discography', releases_path, :class => current_class?(releases_path) %></li>
      <% if current_user %>
        <li><%= link_to 'Account', user_path(current_user), :class => current_class?(user_path(current_user)) %></li>
        <li><%= link_to 'Log Out', destroy_user_session_path, method: :delete %></li>
      <% else %>
