        <li><%= link_to 'Register', new_user_registration_path, :class => current_class?(new_user_registration_path) %></li>
      <% end %>
    </ul>
  </nav>
  <div id="clip"><h1><%= link_to 'Vaporcake', new_question_path %></h1></div>
