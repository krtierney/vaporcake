<div id="new-question-form">
<h1><%= @display.content %></h1>

<%= render 'form', question: @question %>
</div>
<div id="zalgo_txt" >
  <% @responses.each do |question| %>
    <%= question.content %>
  <% end %>
  <!-- questions_asked has many duplicates, need to filter for uniqueness somehow -->
  <% current_user.questions_asked.each do |question| %>
    <%= question.content %>
  <% end %>
</div>

<% if @responses.present? %>
  <div class="glitch-this zalgo first"></div>
  <div class="glitch-this zalgo second"></div>
  <div class="glitch-this zalgo third"></div>
<% end %>